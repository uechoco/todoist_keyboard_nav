{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "short_name": "Todoist key nav",
  "version": "0.0.4",

  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "icons": {
    "128": "icon_128.png",
