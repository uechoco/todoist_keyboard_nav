{
  "manifest_version": 2,
  "name": "Todoist keyboard navigator",
  "version": "0.0.1",

  "description": "keyboard navigation for Todoist web",
  "icons": {
    "128": "icon_128.png",
    "256": "icon_256.png",
    "48": "icon_48.png"
  },
  "author": "uechoco",

  "content_scripts": [
    {
      "js": [ "js/content.js" ],
      "matches": [ "https://todoist.com/app*" ],
      "run_at": "document_start"
    }
  ]
}